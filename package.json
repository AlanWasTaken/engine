{
  "name": "famous-webgl-renderables",
  "version": "0.3.3",
  "description": "Famous WebGL renderables",
  "main": "src/index.js",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "watch-tests": "nodemon test/*.js --exec 'npm run test'",
    "watch": "nodemon --ignore node_modules/ -e js,json --exec 'npm test'",
    "cover": "browserify -t coverify test/*.js | node | coverify",
    "lint": "jshint --reporter node_modules/jshint-stylish/stylish.js src; true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Famous/webgl-renderables.git"
  },
  "author": "",
  "license": "All rights reserved",
  "bugs": {
    "url": "https://github.com/Famous/webgl-renderables/issues"
  },
  "homepage": "https://github.com/Famous/webgl-renderables",
  "testling": {
    "files": [
      "tests/**.spec.js"
    ],
    "scripts": "tests/utilities/bind-polyfill.js"
  },
  "devDependencies": {
    "browserify": "^8.1.0",
    "jshint": "^2.5.11",
    "jshint-stylish": "^1.0.0",
    "nodemon": "^1.3.2",
    "tap-spec": "^2.2.0",
    "tape": "^3.0.3",
    "coverify": "^1.1.0",
    "testling": "^1.7.1",
    "watchify": "^2.1.1"
  },
  "dependencies": {
    "famous-webgl-geometries": "git+ssh://git@github.famo.us/platform/webgl-geometries#v0.1.3",
    "famous-transitions": "git+ssh://git@github.famo.us/platform/transitions#v0.4.1"
  }
}
